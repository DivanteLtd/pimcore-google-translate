{
  "name": "divante-ltd/pimcore-google-translate",
  "type": "pimcore-bundle",
  "license": "GPL-3.0-or-later",
  "description": "Pimcore Google Translate Bundle",
  "keywords": [
    "pimcore",
    "pimcore-plugin",
    "google translate"
  ],
  "homepage": "https://www.divante.com",
  "authors": [
    {
      "name": "Łukasz Marszałek",
      "email": "lmarszalek@divante.pl",
      "role": "Developer"
    },
    {
      "name": "Bruno Ramalho",
      "email": "bramalho@divante.pl",
      "role": "Developer"
    }
  ],
  "require": {
    "pimcore/pimcore": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "symfony/phpunit-bridge": "^4.2",
    "divante-ltd/pimcore-coding-standards": "^0.1",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^4.1",
    "block8/php-docblock-checker": "^1.10",
    "phploc/phploc": "^4.0.0",
    "codeception/codeception": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "GoogleTranslateBundle\\": "src/GoogleTranslateBundle"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "GoogleTranslateBundle\\GoogleTranslateBundle"
      ]
    }
  }
}
